class Solution {
    public String toLowerCase(String str) {
        String s = "";
        for(int i = 0; i < str.length(); i++){
            if(str.charAt(i) >= 'A' && 'Z' >= str.charAt(i))
                s += (char)(str.charAt(i) + 32);
            else
                s += str.charAt(i);
        }
        return s;
    }
}
